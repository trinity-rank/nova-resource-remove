{
    "name": "trinityrank/nova-resource-remove",
    "description": "LaravelNovaResourceRemove - safe remove resource which is related to the another table and assign to another resource",
    "keywords": [
        "laravel",
        "nova",
        "trinityrank",
        "nova-resource-remove"
    ],
    "homepage": "https://github.com/trinity-rank/nova-resource-remove",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "TrinityRank DevTeam",
            "email": "trinitydevbg@gmail.com",
            "role": "Team"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Trinityrank\\LaravelNovaResourceRemove\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Trinityrank\\LaravelNovaResourceRemove\\NovaResourceRemoveServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
